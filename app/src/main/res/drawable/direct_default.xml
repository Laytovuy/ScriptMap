<?xml version="1.0" encoding="utf-8" ?>
<layer-list xmlns:android="http://schemas.android.com/apk/res/android">
    <item android:id="@android:id/background">
        <shape android:shape="line">
            <stroke android:width="2dip" android:color="@color/colorTint_Button" />
        </shape>
    </item>

    <item android:id="@android:id/progress">
        <clip>
            <shape android:shape="line">
                <stroke android:width="2dip" android:color="@color/colorNew_ScriptMap"
                    android:dashWidth="20dp" android:dashGap="4dp"/>
            </shape>
        </clip>
    </item>
</layer-list>